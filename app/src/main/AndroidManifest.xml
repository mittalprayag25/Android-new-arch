<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.prayag.arch">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.prayag.arch.application.CountdownApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">



        <activity android:name="com.prayag.arch.event.ui.EventListActivity">

        </activity>
        <activity android:name=".daggerexample.ui.DaggerActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.prayag.arch.event.ui.AddEventActivity"
            android:windowSoftInputMode="adjustResize" />
    </application>

</manifest>